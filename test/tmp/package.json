{
  "scripts": {
    "lint": "eslint ./*.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": "true"
    },
    "plugins": [
      "privacy"
    ],
    "rules": {
      "no-underscore-dangle": false,
      "privacy/no-access": [
        2,
        "class-only"
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "^3.1.14",
    "eslint": "^0.22.1"
  }
}